{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "strictPropertyInitialization": false,
        "strictNullChecks": false,
        "sourceMap": true,
        "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
    }
}
